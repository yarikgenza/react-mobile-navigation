{
  "name": "react-mobile-navigation-sheet",
  "version": "0.0.1-rc.1",
  "description": "React mobile navigation sheet",
  "author": {
    "name": "Rostyslav Khanas",
    "email": "rk@try.cards"
  },
  "repository": {
    "type": "git"
  },
  "scripts": {
    "storybook-web-build": "cd ./storybooks/web && npm run build-storybook",
    "storybook-web": "cd ./storybooks/web && npm run storybook",
    "storybook-native-watch": "babel ./src --out-dir=./storybooks/native/lib --watch",
    "storybook-native": "cd ./storybooks/native && npm run storybook",
    "storybook-emulator-ios": "cd ./storybooks/native && react-native run-ios",
    "storybook-ios": "npm run storybook-native-watch & npm run storybook-native & npm run storybook-emulator-ios",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "build": "npm run lint && npm run clean && babel src --out-dir lib",
    "start": "cd examples/main && webpack-dev-server --inline --hot"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "invariant": "^2.2.1",
    "react-mobile-navigation-core": "^0.0.1-rc.0"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-motion": "^0.4.4",
    "styled-components": "^1.1.2"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.2.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-motion": "^0.4.4",
    "rimraf": "^2.5.4",
    "styled-components": "^1.1.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
